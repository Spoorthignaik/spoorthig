package com.xworkz.programs;

public class Reverse {

	public static void main(String[] args) {

		String name = "good morning";
		String[] arr = name.split(" ");
		String rev=" ";

		for (int i = arr.length-1; i >=0 ; i--) {
			rev= arr[i]+rev;

				
				}
				System.out.print(" "+rev);
			}
			
		

	}


